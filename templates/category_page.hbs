						<h4 class="govuk-fieldset__heading govuk-!-font-size-19">Filter by:</h4>
					</legend>
					{{#each sections}}
						<div class="govuk-checkboxes govuk-checkboxes--small">
							<div class="govuk-checkboxes__item">
								<input class="govuk-checkboxes__input govuk-!-padding-bottom-2 govuk-!-padding-top-2" id="{{id}}checkbox" name="{{id}}checkbox" type="checkbox" value="{{name}}" checked />
								<label class="govuk-label govuk-checkboxes__label govuk-!-padding-bottom-2 govuk-!-padding-top-2" for="{{id}}checkbox">{{name}}</label>
							</div>
						</div>
					{{/each}}
        </fieldset>
      </div>
    </div>
    <div class="govuk-grid-column-three-quarters">
      <div class="govuk-!-margin-bottom-2 govuk-!-margin-top-9" id="sspinner">
        <div class="spinner">
          <p class="govuk-body govuk-!-font-weight-bold govuk-!-margin-bottom-1">Loading results&hellip;</p>
          <div class="spinner__animation"></div>
        </div>
      </div>
      <div class="govuk-!-margin-bottom-2" id="sresults"></div>
      <div data-apply-gds="">
        <nav class="pagination">
          <ul class="pagination-list govuk-list govuk-list--bullet">
            <li class="pagination-prev" style="display: none;">
